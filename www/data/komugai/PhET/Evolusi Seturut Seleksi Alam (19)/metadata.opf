        <dc:title>Evolusi Seturut Seleksi Alam</dc:title>
        <dc:creator opf:file-as="PhET" opf:role="aut">PhET</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (3.48.0) [https://calibre-ebook.com]</dc:contributor>
